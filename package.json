{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend",
  "main": "dist/index.js",
  "scripts": {
    "server": "nodemon -L -r dotenv/config --exec ts-node src/index.ts",
    "build": "tsc",
    "debug": "ts-node-dev -r dotenv/config --cls --inspect=0.0.0.0:10000 --trace-warnings -- ./src/index.ts",
    "lint": "eslint --ext .ts -f table src || exit 0",
    "lint:errors": "eslint --ext .ts -f table --quiet src || exit 0",
    "fix": "eslint --fix --ext .ts -f table src",
    "format": "npm run lint -s && npm run fix -s"
  },
  "author": "Filip Jaskulski <filip.jaskulski.job@gmail.com>",
  "license": "UNLICENSED",
  "volta": {
    "node": "18.12.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.8.9",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-formatter-table": "^7.32.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^3.1.0",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.438.0",
    "@aws-sdk/lib-dynamodb": "^3.438.0",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.4",
    "amqplib": "^0.10.4",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint-config-airbnb-base": "^15.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2"
  }
}
